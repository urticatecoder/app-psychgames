{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "importsNotUsedAsValues": "error",
    "resolveJsonModule": true,
    "strict": true,
    "isolatedModules": true,
    "declaration": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "removeComments": true
  }
}
